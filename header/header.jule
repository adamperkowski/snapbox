use maps "std/maps"

type HeaderMap = map[str]str

fn Slice(headers: HeaderMap): []str {
	let mut slice: []str = nil

	for _, key in maps::Keys(headers) {
		slice = append(slice, key)
		slice = append(slice, headers[key])
	}

	ret slice
}