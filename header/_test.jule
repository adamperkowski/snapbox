use s "std/slices"
use "std/testing"

#test
fn slicing(t: &testing::T) {
	let headerMap: HeaderMap = {
		ACCEPT: "application/json",
		USER_AGENT: "snapbox",
	}

	let expected: []str = [
		"User-Agent", "snapbox",
		"Accept", "application/json",
	]

	sliced := Slice(headerMap)

	t.Assert(s::Equal(sliced, expected), "sliced HeaderMap does not match expected")
}